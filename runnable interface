package com.company;

class Person2 implements Runnable {
    public void run(){
        for(int i=0;i<5;i++){
            System.out.println(i);

        }
    }

    public static void main(String[] args) {
        Person2 t1=new Person2();
        Thread obj=new Thread(t1);
        obj.start();
    }

}
