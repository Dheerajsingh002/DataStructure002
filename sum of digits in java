package com.company;

import java.util.Scanner;

public class Practice7 {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int m,sum=0;
        while(n!=0){
            m=n%10;
            sum=sum+m;
            n=n/10;
        }
        System.out.println(sum);
    }

}
